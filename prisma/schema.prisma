datasource db {
  provider = "sqlite" // VocÃª pode usar o banco de dados de sua escolha
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Publi {
  id        Int      @id @default(autoincrement())
  titulo    String
  titulo2   String
  titulo4   String
  texto     String
  imagem1   String
  video     String?
  imagem2   String?
  autor     String
  tempo     String
  data      DateTime @map("data") 
  texto2    String
  texto3    String
  texto4    String?
  resumo    String?
  imagem4   String?
}
